$('#photos').html("<%= j render 'photos_list' %>")
<% if @rug.photos.blank? %>
  $('#photo_check').hide();
<% end %>

<% if !@rug.active && !@rug.price.blank? && !@rug.listing_name.blank? && !@rug.photos.blank? && !@rug.location_type.blank? %>
    $('#publish_button').attr('disabled', false);
<% else %>
    $('#publish_button').attr('disabled', true);
<% end %>